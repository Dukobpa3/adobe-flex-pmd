        <artifactId>maven-license-plugin</artifactId>
        <version>1.4.0</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>check</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <header>../flex-pmd-parent/src/etc/header.txt</header>
          <includes>
            <include>**/src/**</include>
            <include>**/test/**</include>
          </includes>
          <excludes>
            <exclude>**/Simple.as</exclude>
            <exclude>**/header.txt</exclude>
            <exclude>**/MainWithNoCopyright.mxml</exclude>
          </excludes>
          <aggregate>true</aggregate>
          <properties>
            <year>2008</year>
            <company>Adobe Systems Incorporated</company>
          </properties>
          <mapping>
            <mxml>XML_STYLE</mxml>
            <xsl>XML_STYLE</xsl>
            <as>JAVADOC_STYLE</as>
          </mapping>
        </configuration>
      </plugin>
